<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Accordion?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<VBox alignment="TOP_CENTER" prefHeight="640.0" prefWidth="1065.0" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/23.0.1" fx:controller="com.example.lifeisagame.HelloController">
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
    </padding>
   <VBox prefHeight="200.0" prefWidth="100.0">
      <children>
         <Label alignment="CENTER" prefHeight="51.0" prefWidth="1028.0" text="Life is a game, let' s play" textAlignment="RIGHT">
            <font>
               <Font size="43.0" />
            </font>
         </Label>
      </children>
   </VBox>

    <Label fx:id="welcomeText" />
   <Accordion />
    <Button onAction="#onHelloButtonClick" text="Hello!" />
</VBox>
